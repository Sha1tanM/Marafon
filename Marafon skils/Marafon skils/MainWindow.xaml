<Window x:Class="Marafon_skils.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Marafon_skils"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <RichTextBox Margin="0,0,0,335">
            <FlowDocument>
                <Paragraph>
                    <Run Text="RichTextBox"/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>
        <Rectangle HorizontalAlignment="Center" Height="110" Margin="0,324,0,0" Stroke="Black" VerticalAlignment="Top" Width="800"/>
        <Button Content="Button" HorizontalAlignment="Center" Margin="0,239,0,0" VerticalAlignment="Top" Height="53" Width="182"/>

    </Grid>
</Window>
